{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/surge/provision-number.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/surge/status.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/sms/webhook.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/sms/send.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/surge/provision-number",
      "destination": "/api/surge/provision-number.js"
    },
    {
      "source": "/api/surge/status",
      "destination": "/api/surge/status.js"
    },
    {
      "source": "/api/sms/webhook",
      "destination": "/api/sms/webhook.js"
    },
    {
      "source": "/api/sms/send",
      "destination": "/api/sms/send.js"
    },
    {
      "source": "/api/subscription/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/api/auth/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/api/stripe/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/api/automation/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/api/reviews",
      "destination": "/server.js"
    },
    {
      "source": "/api/reviews/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/api/sequences",
      "destination": "/server.js"
    },
    {
      "source": "/api/customers",
      "destination": "/server.js"
    },
    {
      "source": "/api/health",
      "destination": "/server.js"
    },
    {
      "source": "/api/feedback-form-settings",
      "destination": "/server.js"
    },
    {
      "source": "/api/private-feedback",
      "destination": "/server.js"
    },
    {
      "source": "/api/private-feedback/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/api/qr-feedback",
      "destination": "/server.js"
    },
    {
      "source": "/api/send-followup-email",
      "destination": "/server.js"
    },
    {
      "source": "/api/send-test",
      "destination": "/server.js"
    },
    {
      "source": "/api/email-track/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/api/ai/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/api/customers/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/api/business/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/api/sequences/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/api/templates/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/api/feedback/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/api/crm/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/api/webhook/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/api/zapier/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/api/cron/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/api/csv/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/api/social/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/api/qr/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/api/r/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/api/techs",
      "destination": "/server.js"
    },
    {
      "source": "/api/checkout/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/api/billing/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/api/profile/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/api/email-track/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/api/active-sequences/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/api/review-requests/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/api/qbo/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/api/google/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/api/automation-executor",
      "destination": "/server.js"
    },
            {
              "source": "/api/qbo-oauth-callback",
              "destination": "/server.js"
            },
            {
              "source": "/api/test-qbo-callback",
              "destination": "/api/test-qbo-callback.js"
            },
            {
              "source": "/api/test-qbo-customers",
              "destination": "/server.js"
            },
            {
              "source": "/api/test-db",
              "destination": "/server.js"
            },
    {
      "source": "/auth/callback",
      "destination": "/index.html"
    },
    {
      "source": "/post-checkout",
      "destination": "/index.html"
    },
    {
      "source": "/settings",
      "destination": "/index.html"
    },
    {
      "source": "/settings/billing",
      "destination": "/index.html"
    },
    {
      "source": "/settings/business-profile",
      "destination": "/index.html"
    },
    {
      "source": "/settings/team-roles",
      "destination": "/index.html"
    },
    {
      "source": "/settings/audit-log",
      "destination": "/index.html"
    },
    {
      "source": "/settings/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*\\.js)$",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/assets/(.*\\.css)$",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "crons": [
    {
      "path": "/api/automation-executor",
      "schedule": "*/1 * * * *"
    },
    {
      "path": "/api/cron/refresh-qbo-tokens",
      "schedule": "0 2 * * *"
    }
  ]
}